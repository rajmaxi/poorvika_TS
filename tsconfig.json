{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "noImplicitAny": false,  "skipLibCheck": true,    "target": "esnext",
   
    "typeRoots": [
      "src/customTypings",
      "node_modules/@types"
    ],
    "baseUrl": "./",
    /* Base directory to resolve non-absolute module names. */
    "paths": {
      "src": ["./src/*"],
      "screens": ["./src/screens/*"],
      "components": ["./src/components/*"],
      "constants": ["./src/constants/*"],
      "locale": ["./src/locale/*"],
      "navigation": ["./src/navigation/*"],
      "utils":["./src/utils/*"],
      "store":["./src/store/*"],
      "reducer": ["./src/reducer/*"],
      "saga": ["./src/saga/*"],
      "common-components": ["./common-components"],
      "state": ["./state"]
    },
    
  },
  "include": ["src"],
  "exclude": [
    "node_modules",
    "typings",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
    
  ]
}