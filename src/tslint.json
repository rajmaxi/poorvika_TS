{
    "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
    "rules": {
      "variable-name": [true, "allow-leading-underscore"],
      "no-console": false,
      "no-submodule-imports": [
        true,
        "source-map-support",
        "module-alias",
        "@global",
        "@milestone",
        "@config",
        "@services"
      ],
      "no-implicit-dependencies": [true, "dev", ["@global", "@milestone", "@screens", "@services"]],
      "no-boolean-literal-compare": false
    }